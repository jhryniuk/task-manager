<VirtualHost *:80>
    ServerAdmin webmaster@task-manager.dev
    DocumentRoot /var/www/task-manager
    ServerName task-manager.dev
    ServerAlias www.task-manager.dev

    <Directory "/var/www/task-manager">
        Options Indexes FollowSymLinks

        AllowOverride All

        Order allow,deny
        Allow from all
    </Directory>

    ErrorLog  /var/log/apache2/task-manager-error_log
    CustomLog /var/log/apache2/task-manager-access_log common
</VirtualHost>
